{
  "name": "aipe",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "@types/uuid": "^9.0.1",
    "openai": "^3.2.1",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0",
    "vite": "^4.2.0"
  },
  "type": "module",
  "dependencies": {
    "@types/node": "^18.16.1",
    "mongodb": "^5.3.0",
    "ts-node": "^10.9.1"
  },
  "prettier": {
    "printWidth": 80,
    "overrides": [
      {
        "files": [
          "*.svelte"
        ],
        "options": {
          "printWidth": 100,
          "tabWidth": 3
        }
      }
    ]
  }
}
