{
  "name": "aipe",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest src",
    "test:watch": "npm run test -- --watch",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-typescript": "^7.23.3",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/svelte": "^4.0.5",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.10",
    "@types/uuid": "^9.0.1",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "openai": "^3.2.1",
    "postcss": "^8.4.31",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "svelte-jester": "^3.0.0",
    "tailwindcss": "^3.3.5",
    "ts-jest": "^29.1.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0",
    "vite": "^4.2.0",
    "zod": "^3.22.4"
  },
  "type": "module",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@types/node": "^18.16.1",
    "mongodb": "^5.3.0",
    "ts-node": "^10.9.1"
  },
  "jest": {
    "transform": {
      "^.+\\.[j|t]sx?$": "ts-jest",
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ]
    },
    "extensionsToTreatAsEsm": [".svelte"]
  },
  "prettier": {
    "printWidth": 85,
    "overrides": [
      {
        "files": [
          "*.svelte"
        ],
        "options": {
          "printWidth": 130,
          "tabWidth": 3
        }
      }
    ]
  }
}
